<h1>Articles</h1>
<% @articles.each do |article| %>
  <p><%= article.title %></p>
  <%= link_to "see fact", article_path(article) %>
  <%= link_to "edit article", edit_article_path(article) %>
  <%= link_to "delete", article_path(article), method: :delete %>
<% end %>
<br>
<h3>Create Article <%= link_to "here", new_article_path %></h3>

